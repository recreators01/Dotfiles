{
  "log": {
    "disabled": true,
    "level": "info",
    "output": "singbox.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "type": "local",
        "tag": "dns_local"
      },
      {
        "type": "tls",
        "tag": "dns_proxy",
        "server": "1.1.1.1"
      },
      {
        "type": "tls",
        "tag": "dns_direct",
        "domain_resolver": "dns_local",
        "server": "dot.pub"
      },
      {
        "type": "fakeip",
        "tag": "dns_fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "rule_set": "category-games@cn",
        "server": "dns_direct"
      },
      {
        "rule_set": "AWAvenue-Ads",
        "action": "reject"
      },
      {
        "rule_set": "cn-domain",
        "server": "dns_direct"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip"
      }
    ],
    "final": "dns_proxy",
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 7890
    },
    {
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "🚀 节点选择",
      "outbounds": [
        "🇺🇸 美国节点",
        "🇭🇰 香港节点",
        "🇹🇼 台湾节点",
        "🇯🇵 日本节点",
        "🇸🇬 新加坡节点"
      ],
      "providers": null,
      "interrupt_exist_connections": true
    },
    {
      "type": "urltest",
      "tag": "🇭🇰 香港节点",
      "outbounds": null,
      "providers": [
        "🛫 机场订阅"
      ],
      "include": "(?i)(🇭🇰|港|hk|hongkong|hong kong)"
    },
    {
      "type": "urltest",
      "tag": "🇹🇼 台湾节点",
      "outbounds": null,
      "providers": [
        "🛫 机场订阅"
      ],
      "include": "(?i)(🇹🇼|台|tw|taiwan|tai wan)"
    },
    {
      "type": "urltest",
      "tag": "🇯🇵 日本节点",
      "outbounds": null,
      "providers": [
        "🛫 机场订阅"
      ],
      "include": "(?i)(🇯🇵|日|jp|japan)"
    },
    {
      "type": "urltest",
      "tag": "🇸🇬 新加坡节点",
      "outbounds": null,
      "providers": [
        "🛫 机场订阅"
      ],
      "include": "(?i)(🇸🇬|新|sg|singapore)"
    },
    {
      "type": "urltest",
      "tag": "🇺🇸 美国节点",
      "outbounds": null,
      "providers": [
        "🛫 机场订阅"
      ],
      "include": "(?i)(🇺🇸|美|us|unitedstates|united states)"
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "providers": [
    {
      "type": "remote",
      "tag": "🛫 机场订阅",
      "url": "",
      "update_interval": "24h0m0s"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "rule_set": "category-games@cn",
        "outbound": "direct"
      },
      {
        "rule_set": "AWAvenue-Ads",
        "action": "reject"
      },
      {
        "rule_set": "cn-domain",
        "outbound": "direct"
      },
      {
        "rule_set": "cn-ip",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "AWAvenue-Ads",
        "url": "https://raw.githubusercontent.com/TG-Twilight/AWAvenue-Ads-Rule/main/Filters/AWAvenue-Ads-Rule-Singbox.srs",
        "update_interval": "24h0m0s"
      },
      {
        "type": "remote",
        "tag": "category-games@cn",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-games@cn.srs",
        "update_interval": "24h0m0s"
      },
      {
        "type": "remote",
        "tag": "geolocation-!cn",
        "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-geolocation-!cn.srs",
        "update_interval": "24h0m0s"
      },
      {
        "type": "remote",
        "tag": "cn-domain",
        "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-cn.srs",
        "update_interval": "24h0m0s"
      },
      {
        "type": "remote",
        "tag": "cn-ip",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs",
        "update_interval": "240h0m0s"
      }
    ],
    "final": "🚀 节点选择",
    "auto_detect_interface": true,
    "default_domain_resolver": "dns_direct"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "zashboard",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/archive/refs/heads/gh-pages.zip",
      "default_mode": "Rule"
    }
  }
}
